Covariate Correction Script

Apollo <- read.csv("EverythingSwitched.csv", header = TRUE)
a <- Apollo$PMI
b <- Apollo$pH
c <- Apollo$RIN

fit <- c()
for(i in 44:16358){

fit[i] <- residuals(lm(Apollo[,i]~a+b+c, data = Apollo))
}Athena <- matrix(fit[i], 420, byrow = TRUE)write.csv(fit.resid, "CovariateCorrected.csv", row.names = FALSE)
